MiniWindowHeadLess
  id: minimapWindow
  height: 150
  @onClose: modules.game_minimap.onMiniWindowClose()
  &save: true

  MiniWindowContentsHL
    Minimap
      margin: 1 1 1 3
      id: minimap
      anchors.fill: parent

  ResizeBorder
    anchors.right: parent.right
    anchors.top: parent.top
    anchors.bottom: parent.bottom
    enabled: true
