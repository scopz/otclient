MiniWindowNoBar
  id: minimapWindow
  height: 150
  @onOpen: modules.game_minimap.onMiniWindowOpen()
  &save: true

  MiniWindowNoBarContents
    Minimap
      margin: 1 1 1 3
      id: minimap
      anchors.fill: parent

  ResizeBorder
    anchors.right: parent.right
    anchors.top: parent.top
    anchors.bottom: parent.bottom
    enabled: true
